{
  "name": "terra-classic-linktree",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && SSR_TARGET=node vite build --ssr src/ssr.tsx",
    "build:pages": "tsc && CF_PAGES_BUILD=true vite build && SSR_TARGET=webworker CF_PAGES_BUILD=true vite build --ssr src/entry-worker.ts",
    "start": "NODE_ENV=production node server.js",
    "preview": "vite preview",
    "serve": "NODE_ENV=production node server.js",
    "dev:ssr": "NODE_ENV=development node server.js",
    "deploy:pages": "yarn build:pages && wrangler pages deploy dist",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "check": "tsc --noEmit && eslint src --ext ts,tsx"
  },
  "dependencies": {
    "compression": "^1.8.0",
    "express": "^4.18.2",
    "lucide-react": "^0.270.0",
    "d3-color": "^3.1.0",
    "d3-delaunay": "^6.0.4",
    "d3-force": "^3.0.0",
    "d3-interpolate": "^3.0.1",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.3",
    "remark-gfm": "^4.0.1",
    "sirv": "^3.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-node-polyfills": "^0.15.0",
    "vite-plugin-ssr": "^0.4.142"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/d3-delaunay": "^6.0.4",
    "@types/d3-force": "^3.0.6",
    "@types/d3-selection": "^3.0.8",
    "@types/d3-zoom": "^3.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.2.1",
    "@cloudflare/vite-plugin": "^1.13.7",
    "@cloudflare/workers-types": "^4.20250927.0",
    "autoprefixer": "^10.0.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0",
    "web-vitals": "^5.0.1",
    "wrangler": "^4.40.2"
  }
}
